{
  "name": "Font Contrast",
  "description": "Improves webpage readability.",
  "version": "1.5.9",
  "author": "Fushko",
  "background": {
    "service_worker": "src/background.js"
  },
  "permissions": [
    "storage", "tabs", "activeTab", "scripting"
  ],
  "host_permissions": [
    "*://*/*"
  ],
  "action": {
    "default_popup": "pageAction_popup.html",
    "default_icon": {
      "16": "assets/icons/on.png",
      "48": "assets/icons/on.png",
      "128": "assets/icons/on.png"
    }
  },
  "options_ui": {
    "page": "options.html",
    "browser_style": false,
    "open_in_tab": false
},
  "icons": {
    "16": "assets/icons/on.png",
    "48": "assets/icons/on.png",
    "128": "assets/icons/on.png"
  },
  "manifest_version": 3,
  "commands": {
	"toggle": {
		"suggested_key": {
			"default": "Ctrl+Shift+5"
		},
		"description": "Toggle contrast fix"
	}
  }
}
